    "definitions": {
        "handler.CreateAccountInput": {
            "type": "object",
            "required": [
                "currency_code",
                "name"
            ],
            "properties": {
                "currency_code": {
                    "type": "string"
                },
                "initial_balance": {
                    "type": "number"
                },
                "name": {
                    "type": "string"
                }
            }
        },
        "handler.LoginInput": {
            "type": "object",
            "required": [
                "email",
                "password"
            ],
            "properties": {
                "email": {
                    "type": "string"
                },
                "password": {
                    "type": "string"
                }
            }
        },
        "handler.RegisterInput": {
            "type": "object",
            "required": [
                "email",
                "password"
            ],
            "properties": {
                "email": {
                    "type": "string"
                },
                "full_name": {
                    "type": "string"
                },
                "password": {
                    "type": "string",
                    "minLength": 6
                }
            }
        },
        "handler.updateAccountRequest": {
            "type": "object",
            "properties": {
                "currency_code": {
                    "type": "string"
                },
                "initial_balance": {
                    "type": "number"
                },
                "name": {
                    "type": "string"
                }
            }
        },
        "model.CategorySpending": {
            "type": "object",
            "properties": {
                "category_name": {
                    "type": "string"
                },
                "percentage": {
                    "description": "Будет вычислено в сервисе",
                    "type": "number"
                },
                "total_amount": {
                    "type": "number"
                },
                "transaction_count": {
                    "type": "integer"
                }
            }
        },
        "model.FinancialHealth": {
            "type": "object",
            "properties": {
                "health_score": {
                    "type": "integer"
                },
                "income_expense_ratio": {
                    "type": "number"
                },
                "largest_expense_category": {
                    "type": "string"
                },
                "monthly_trends": {
                    "type": "array",
                    "items": {
                        "$ref": "#/definitions/model.MonthlyBalanceTrend"
                    }
                },
                "total_balance": {
                    "type": "number"
                }
            }
        },
        "model.MonthlyBalanceTrend": {
            "type": "object",
            "properties": {
                "balance": {
                    "type": "number"
                },
                "month": {
                    "description": "Формат: \"Январь 2024\"",
                    "type": "string"
                }
            }
        }
